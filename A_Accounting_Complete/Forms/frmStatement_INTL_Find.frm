Dim RsFind As ADODB.Recordset
Dim SQL As String

Private Sub cmdExit_Click()
Unload Me
End Sub

Private Sub cmdInsert_Click()
On Error GoTo ErrExit
Dim ask As Integer
ask = MsgBox("Load this SA#? " & Me.DataGrid1.Columns(1).Text, vbInformation + vbYesNo)

If ask = vbYes Then
       With frmStatement_INTL
               .LoadValuesSA Me.DataGrid1.Columns(0).Text
               .cmdNew.Enabled = False
               .cmdPost.Enabled = True
               .cmdCancel.Enabled = True
               .cmdFind.Enabled = True
               .cmdOVerRide.Enabled = False
               .Frame1.Caption = "over-ride"
      End With
End If

Exit Sub
ErrExit:
Select Case Err.Number
Case 6160
    MsgBox "There are no PO(s) to Insert!", vbCritical
End Select
End Sub

Private Sub Form_Load()

Set RsFind = New ADODB.Recordset
SQL = "SELECT * FROM tbl_Statement_INTL ORDER by [SAno] ASC"
RsFind.Open SQL, cn, adOpenKeyset, adLockOptimistic

Set Me.DataGrid1.DataSource = RsFind

End Sub


Private Sub txtSearch_Change()
DisplayGrid Me.txtSearch
End Sub

Sub DisplayGrid(Param)
Set RsFind = New ADODB.Recordset
    SQL = "SELECT * FROM tbl_Statement_INTL WHERE [SAno] like '" & Param & "%'"
    
    RsFind.Open SQL, cn, adOpenKeyset, adLockOptimistic
    Set Me.DataGrid1.DataSource = RsFind
    Me.DataGrid1.Refresh

End Sub
